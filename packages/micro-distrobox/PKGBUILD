pkgname=micro-distrobox
pkgver=1.4.2.2
pkgrel=2
pkgdesc="Another tool for containerized command line environments on Linux"
arch=('any')
url="https://distrobox.privatedns.org/"
license=('GPL3')
source=("$pkgname::git+https://github.com/koompi-os/micro-distrobox.git")

md5sums=('SKIP')


package() {
  cd "$srcdir/$pkgname"
  mkdir -p "$pkgdir/usr"
  cp -r usr/* "$pkgdir/usr/"
}

